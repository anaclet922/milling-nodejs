    <script>

        $(document).ready(function(){

            <% if(message.error){ %>
                showToastSuccessOrErrorJS('error', '<%= message.error %>')
            <% } %>

            <% if(message.success){ %>
                showToastSuccessOrErrorJS('success', '<%= message.success %>')
            <% } %>
        });
      
        function showToastSuccessOrErrorJS(icon, message) {
            const Toast = Swal.mixin({
                toast: true,
                position: 'top-end',
                showConfirmButton: false,
                timer: 4000,
                timerProgressBar: true,
                didOpen: (toast) => {
                    toast.addEventListener('mouseenter', Swal.stopTimer)
                    toast.addEventListener('mouseleave', Swal.resumeTimer)
                }
            })

            Toast.fire({
                icon: icon,
                title: message
            })

        }

